
@{
	Layout = null;
}

<!DOCTYPE html>
<html>
<head>
	<title>Welcome To Chandan Hospital</title>
	<meta charset="utf-8">
	<meta name="theme-color" content="#1c82d7" />
	<!-- Windows Phone -->
	<meta name="msapplication-navbutton-color" content="#1c82d7" />
	<!-- iOS Safari -->
	<meta name="apple-mobile-web-app-status-bar-style" content="#1c82d7" />
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	<link href="~/Content/css/custom.css?v1.0" rel="stylesheet" />
	<script src="~/JsModule/global.js"></script>
	<link href="~/Content/css/select2.css" rel="stylesheet" />
	<script src="~/Content/js/select2.js"></script>
	<style>
		#btnBack {
			float: left;
			padding: 1px 7px;
			/*transform: rotate( 180deg );*/
			margin-left: 5px;
		}

		.fa-home {
			cursor: pointer;
			font-size: 28px;
			color: #ec1100;
			margin: 12px 10px;
		}

		#layout-section h2.page-heading {
			font-size: 22px;
			margin: 10px 0;
			color: #444;
		}

		#layout-section {
			background: #f3f3f3;
			padding: 0px 10px;
			border-top: 2px solid #ec1100;
			margin: 5px 0px 0 12px;
			height: 570px;
		}

		.menu-section {
			position: fixed;
			background: #f3f3f3;
			padding: 0;
			border-top: 2px solid #ec1100;
			margin: 5px 0;
			width: 278px;
			left: 0;
		}

			.menu-section h3 {
				color: #01447b;
				text-align: center;
				margin: 0;
			}

			.menu-section ul {
				height: 532px;
				padding: 0;
				list-style: none;
				overflow: auto;
				overflow-y: auto;
			}

				.menu-section ul li {
					cursor: pointer;
					height: 75px;
					width: 125px;
					background: #0473a9;
					color: #fff;
					line-height: 16px;
					margin: 5px 5px;
					display: inline-block;
				}

					.menu-section ul li:hover {
						background: #5f6b71;
						box-shadow: none;
					}

					.menu-section ul li a {
						width: -webkit-fill-available;
						padding: 26px 22px;
						text-align: center;
						display: inline-grid;
						color: #fff;
						text-decoration: none;
						font-weight: 600;
						font-size: 13px;
					}

			.menu-section .fa-bars {
				color: #e31843;
				cursor: pointer;
				right: 12px;
				position: absolute;
			}

		.toggle-bars {
			color: #e31843;
			cursor: pointer;
			background: #fff;
			padding: 5px;
			font-size: 21px;
			top: 6px;
			position: absolute;
			left: 0px;
			display: none;
		}

		body {
			background: #d5e4ea;
		}

		.btn-success {
			background: #0ca526;
		}

		.bn {
			border-radius: 0;
		}

		.panel-body {
			padding: 5px 0px;
		}

		.header h3 {
			font-size: 19px;
			color: #464646;
			margin: 5px;
			font-weight: 600;
		}

		.login {
			box-shadow: 2px 2px 11px #636363;
			margin: 10% 0 0 0;
			background: #ffffffc4;
			padding: 20px;
		}

		.login-heading {
			padding: 0px;
			font-family: sans-serif;
			font-weight: bold;
			color: #2347ad;
			text-align: center;
			margin: 0px 0px 11px 0px;
		}

		.btn-flat {
			border: none;
			border-radius: 1px;
		}

		.left-panel {
			border-right: 1px solid #fff;
		}

		.right-panel {
			font-size: 19px;
			color: #01447b;
			padding: 0;
			align-content: center;
			display: flex;
		}

		.about {
			background: #fff;
			padding: 15px;
			text-align: left;
			margin: 10px 0px 10px 0px;
		}

		.banner {
			background: #fff;
			padding: 3px;
			text-align: left;
			margin: 10px 0px;
		}

		.panels {
			background: #fff;
			padding: 1px 0;
			text-align: left;
			margin: 12px 0 0px 0px;
		}

		.heading {
			color: #005498;
			text-align: center;
			font-size: 22px;
			text-transform: uppercase;
			font-weight: 600;
			text-shadow: 0px 3px 10px #ffa302;
		}

		a.login-btn {
			display: block;
			border-radius: 2px;
			background: #00447b;
			color: #fff;
			padding: 0px 0px;
			margin: 8px 9px;
			text-align: center;
			cursor: pointer;
			text-decoration: none;
		}

			a.login-btn:hover {
				background: #e91e63;
				padding-left: 10px;
				box-shadow: 5px 3px 7px #585555;
			}

		hr {
			margin: 0;
		}

		.footer-heading {
			font-size: 21px;
			margin: 0;
			color: #e91e63;
			font-weight: 600;
		}

		ul.btn-list {
			line-height: 49px;
			padding: 0;
			list-style: none;
		}

		.services-list {
			margin: 0;
			padding: 0;
		}

			.services-list li:hover {
				background: #e91e63;
				padding-left: 10px;
				box-shadow: 5px 3px 7px #585555;
			}

			.services-list li {
				/*box-shadow: 5px 2px 7px #585555;*/
				display: inline-block;
				border-radius: 1px;
				background: #00447b;
				color: #fff;
				font-size: 12px;
				padding: 2px 10px;
				margin: 3px 3px;
				text-align: center;
				cursor: pointer;
			}

		.footer-panel {
			background: #fff;
			border-top: 5px double #ffaa34;
			font-family: sans-serif;
			line-height: 20px;
			padding: 0 20px;
			border-radius: 10px;
		}

			.footer-panel b {
				color: #f15959;
				font-weight: 700;
			}

		.bg-tran {
			padding: 0 5px;
			background: #fff;
		}

		.panel-heading {
			padding: 0px 5px !important;
		}

		.container {
			padding: 0 4px;
			width: 100%;
		}

		.table > caption + thead > tr:first-child > td, .table > caption + thead > tr:first-child > th, .table > colgroup + thead > tr:first-child > td, .table > colgroup + thead > tr:first-child > th, .table > thead:first-child > tr:first-child > td, .table > thead:first-child > tr:first-child > th {
			border-top: 0;
			background: #f2fdff;
			color: #292929;
			padding: 0px 5px;
			font-size: 12px;
		}

		input[type=checkbox] {
			margin: 2px;
			height: 15px;
		}

		.title {
			padding: 1px 5px;
			background: #dedede;
			font-size: 12px;
		}

		.page-box {
			margin-top: 10px;
		}

		.table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
			padding: 0px 5px;
			background: #fff;
			border-top: 1px solid #ddd;
			font-size: 14px;
		}

		.btn-del {
			font-size: 8px;
			padding: 0px 4px;
		}

		.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th {
			vertical-align: middle;
			text-align: left;
			white-space: nowrap;
		}

		.tableFixHead {
			overflow-y: auto;
			height: 190px;
			border: 1px solid #009688;
		}

		.Inactive {
			pointer-events: none;
			opacity: 0.5;
		}

		button {
			font-size: 14px;
		}
	</style>
</head>
<body>
	<div class="container">
		@Html.Partial("_header")
		@RenderBody()
	</div>
</body>
</html>
